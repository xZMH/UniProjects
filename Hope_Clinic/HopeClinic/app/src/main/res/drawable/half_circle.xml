<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <solid android:color="#00046500"/>
            <size
                android:width="130dp"
                android:height="120dp"/>
        </shape>
    </item>

    <item android:bottom="30dp" android:start="-120dp"
        android:top="-30dp">
        <shape
            android:shape="oval">
            <solid android:color="#046500"/>
            <gradient
                android:startColor="#5BED00"
                android:centerColor="#00A800"
                android:endColor="#157300"
                android:angle="270" />
            <size
                android:width="120dp"
                android:height="70dp"/>
        </shape>
    </item>
</layer-list>